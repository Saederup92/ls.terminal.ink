{
  "name": "forklift",
  "version": "10.0.0",
  "description": "The Express server that hosts the bot directory listing.",
  "main": "index.js",
  "scripts": {
    "test": "eslint .",
    "start": "node --icu-data-dir=node_modules/full-icu website/index.js",
    "bot": "node --icu-data-dir=node_modules/full-icu bot/index.js",
    "sass": "sass --watch src/static/css/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terminal/ls.terminal.ink.git"
  },
  "author": "7coil",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/terminal/ls.terminal.ink/issues"
  },
  "homepage": "https://ls.terminal.ink/",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "discord.js": "^11.4.2",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "flat": "^4.1.0",
    "full-icu": "^1.2.1",
    "github-username-regex": "^1.0.0",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "i18n": "^0.8.3",
    "joi": "^14.0.4",
    "js2xmlparser": "^3.0.0",
    "jsonwebtoken": "^8.4.0",
    "marked": "^0.3.19",
    "monaco-editor": "^0.14.3",
    "multer": "^1.4.1",
    "mustache": "^2.3.2",
    "node-fetch": "^2.3.0",
    "node-sass-middleware": "^0.11.0",
    "passport": "^0.3.2",
    "passport-discord": "^0.1.3",
    "request": "^2.88.0",
    "rethinkdbdash": "^2.3.31",
    "session-rethinkdb": "^2.0.1",
    "sharp": "^0.21.0",
    "tiny-slider": "^2.8.8",
    "valid-url": "^1.0.9",
    "xss": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.14.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "plugins": [
      "import"
    ],
    "rules": {
      "indent": [
        2
      ],
      "no-tabs": 0,
      "comma-dangle": 0,
      "max-len": 0,
      "no-console": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "newline-per-chained-call": 0
    },
    "env": {
      "node": true,
      "browser": true
    }
  }
}
